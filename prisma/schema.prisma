generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Country {
  id         Int      @id @default(autoincrement())
  code       String   @unique
  name       String
  start      DateTime
  end        DateTime?
  presidents Json
  monarchs   Json
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
